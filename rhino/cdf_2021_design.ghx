                  <items count="12">
                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.rpc import Proxy


if compute_displ:
    #wall_methods_numpy = Proxy('window_in_wall.wall_methods_numpy')
    with Proxy('window_in_wall.wall_methods_pde') as wall_methods_pde:
        #wall_methods_numpy.stop_server() #if changes need to be reloaded
        #wall_methods_numpy.start_server()
        
        # compute displacement
        #solX, solZ = wall_methods_numpy.compute_displacement(wall.mesh,wall.gate_points, wall.x_size, wall.z_size)
        solX = wall_methods_pde.compute_displacement_x(wall.mesh, wall.gate_points, wall.x_size, wall.z_size)
        solZ = wall_methods_pde.compute_displacement_z(wall.mesh, wall.gate_points, wall.x_size, wall.z_size)
    
    # update vertex attributes with displacement values
    for v, data in wall.mesh.vertices(data=True):        
        wall.mesh.vertex_attribute(v, "x_disp", value=solX[v]) #value=solX[n_from_ij(i,j, wall.z_size)]
        wall.mesh.vertex_attribute(v, "z_disp", value=solZ[v]) #value=solZ[n_from_ij(i,j, wall.z_size)]

if visualize_displ:
    # displace the vertices (for preview only)
    wall.displace_vertices()
    #solmat = np.reshape(solX, (-1, n))</item>
