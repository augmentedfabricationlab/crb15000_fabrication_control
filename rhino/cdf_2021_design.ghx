                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas_ghpython.artists import MeshArtist
import Rhino.Geometry as rg

rh_mesh = []
vertices_gate = []
vertices_all = []
disp_vectors = []


if show_mesh:
    # Mesh Artist for visualization
    artist = MeshArtist(wall.mesh)
    
    #vertices = artist.draw_vertices()
    #edges = artist.draw_edges()
    #faces = artist.draw_faces()
    #labels = {key: key for key in mesh.vertices()}
    
    rhmesh = artist.draw_mesh()
    
    artist_offset = MeshArtist(wall.mesh_offset)
    rhmesh_offset = artist_offset.draw_mesh()

if show_vertices_gate:
    # draw gate points
    v = []
    for vertex in wall.mesh.vertices():
        gp = wall.mesh.vertex_attribute(vertex, name='in_gate')
        if gp: v.append(vertex)
    
    #print v
    if len(v): vertices_gate = artist.draw_vertices(vertices=v)


if show_vector_field:
    x_disp = []
    z_disp = []
    
    for vertex in wall.mesh.vertices():
        x_disp.append(wall.mesh.vertex_attribute(vertex, name='x_disp'))
        z_disp.append(wall.mesh.vertex_attribute(vertex, name='z_disp'))
    
    disp_vectors = [rg.Vector3d(x_disp[i], 0, z_disp[i])/5 for i in range(len(x_disp))]

    vertices_all = artist.draw_vertices()



