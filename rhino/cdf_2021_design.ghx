                    <item name="CodeInput" type_name="gh_string" type_code="10">import math
import compas
import Rhino as rh
import ghpythonlib.components as ghcomp
from compas.datastructures import Mesh

import Rhino.Geometry as rg

# Create wall curves left and right
wall_crv_left = rg.Line(rg.Point3d(0,0,0), rg.Point3d(0,0,wall.height))
wall_crv_right = rg.Line(rg.Point3d(wall.width,0,0), rg.Point3d(wall.width,0,wall.height))

# --- catenary functions ---
def catenary_y(a,x):
    try:
        ans = a*math.cosh(x/a)
    except OverflowError:
        ans = float('inf')
    return ans

def define_catenary_a(cat_width, cat_height, tol=0.0001):
    length = cat_width/2
    an, an1, da = (1, 1, 0.1)
    hn = catenary_y(an, length)-an
    hn1 = hn
    for i in range(1000):
        dhn = hn-cat_height
        dhn1 = hn1-cat_height
        if (hn1 &gt; cat_height and abs(dhn1)&gt;abs(dhn)) or hn1==float('inf'):
            da = da/10
        if abs(dhn)&lt;tol:
            break
        elif dhn&lt;0:
            an = an-da
            an1 = an-da
        elif dhn&gt;0:
            an = an+da
            an1 = an+da
        hn = catenary_y(an,length)-an
        hn1 = catenary_y(an1,length)-an1
    return an
# --- end of catenary functions ---

if gate_type == "interactive":
    # Create wall curve above gate
    gate_left_pts = ghcomp.EndPoints(curve=gate_crv_left)
    if gate_left_pts[0].Z &gt; gate_left_pts[1].Z:
        gate_top_pt = gate_left_pts[0]
    else:
        gate_top_pt = gate_left_pts[1]
    gate_x = gate_top_pt.X
elif gate_type == "catenary":
    cat_a = define_catenary_a(gate_size, gate_size)

if gate_type=="circular":
    center_pt = rg.Point3d(gate_x,0,0)
    start_pt = rg.Point3d(gate_x-gate_size,0,0)
    middle_left_pt = rg.Point3d(gate_x-(gate_size*(math.sqrt(2)/2)),0,gate_size*(math.sqrt(2)/2))
    middle_pt = rg.Point3d(gate_x,0,gate_size)
    middle_right_pt = rg.Point3d(gate_x+(gate_size*(math.sqrt(2)/2)),0,gate_size*(math.sqrt(2)/2))
    end_pt = rg.Point3d(gate_x+gate_size,0,0)
    gate_crv_left = ghcomp.Arc3Pt(start_pt, middle_left_pt, middle_pt)[0]
    gate_crv_right = ghcomp.Arc3Pt(middle_pt, middle_right_pt, end_pt)[0]
elif gate_type=="triangular":
    start_pt = rg.Point3d(gate_x-gate_size,0,0)
    middle_pt = rg.Point3d(gate_x,0,gate_size)
    end_pt = rg.Point3d(gate_x+gate_size,0,0)
    gate_crv_left = ghcomp.Line(start_pt, middle_pt)
    gate_crv_right = ghcomp.Line(end_pt, middle_pt)
elif gate_type=="persian":
    pers_radius = (1/3)*gate_size
    start_pt = rg.Point3d(gate_x-pers_radius,0,0)
    y1 = pers_radius*(math.sqrt(2.5-math.sqrt(2))+math.sqrt(2)/2)
    arc1_start_pt = rg.Point3d(gate_x-pers_radius,0,y1)
    arc2_start_pt = rg.Point3d(gate_x+pers_radius,0,y1)
    y2 = pers_radius*(math.sqrt(2.5-math.sqrt(2))+math.sqrt(2))
    int_relx = math.sqrt(pers_radius**2-(y2-y1)**2)
    arc1_int_pt = rg.Point3d(gate_x-(int_relx)*(math.sqrt(3)/math.sqrt(2)),0,y1+(y2-y1)/(math.sqrt(2)))
    arc2_int_pt = rg.Point3d(gate_x+(int_relx)*(math.sqrt(3)/math.sqrt(2)),0,y1+(y2-y1)/(math.sqrt(2)))
    
    arc1_end_pt = rg.Point3d(gate_x-int_relx,0,y2)
    arc2_end_pt = rg.Point3d(gate_x+int_relx,0,y2)
    y3 = math.sqrt(4*(pers_radius**2)-int_relx**2)+y2-2*(y2-y1)
    top_pt = rg.Point3d(gate_x,0,y3)
    arc3_int_pt = rg.Point3d(gate_x+int_relx-(2*int_relx)/math.sqrt(2),0,y1-(y2-y1)+2*(y2-y1)*(math.sqrt(3)/math.sqrt(2)))
    arc4_int_pt = rg.Point3d(gate_x-int_relx+(2*int_relx)/math.sqrt(2),0,y1-(y2-y1)+2*(y2-y1)*(math.sqrt(3)/math.sqrt(2)))
    
    end_pt = rg.Point3d(gate_x+pers_radius,0,0)
    gate_vert_left = ghcomp.Line(start_pt, arc1_start_pt)
    gate_vert_right = ghcomp.Line(end_pt, arc2_start_pt)
    arc1_crv = ghcomp.Arc3Pt(arc1_start_pt, arc1_int_pt, arc1_end_pt)
    arc2_crv = ghcomp.Arc3Pt(arc2_start_pt, arc2_int_pt, arc2_end_pt)
    arc3_crv = ghcomp.Arc3Pt(arc1_end_pt, arc3_int_pt, top_pt)
    arc4_crv = ghcomp.Arc3Pt(arc2_end_pt, arc4_int_pt, top_pt)
    
    gate_crv_left = ghcomp.JoinCurves([gate_vert_left, arc1_crv[0], arc3_crv[0]], False)
    gate_crv_right = ghcomp.JoinCurves([gate_vert_right, arc2_crv[0], arc4_crv[0]], False)
elif gate_type=="catenary":
    start_pt = rg.Point3d(gate_x-gate_size/2,0,0)
    end_pt = rg.Point3d(gate_x+gate_size/2,0,0)
    pts_left = [rg.Point3d(gate_x-((gate_size/2)/5)*i, 0, gate_size-catenary_y(cat_a, ((gate_size/2)/5)*i)+cat_a) for i in range(6)]
    pts_right = [rg.Point3d(gate_x+((gate_size/2)/5)*i, 0, gate_size-catenary_y(cat_a, ((gate_size/2)/5)*i)+cat_a) for i in range(6)]
    pts_left[-1] = start_pt
    pts_right[-1] = end_pt
    gate_crv_left = ghcomp.Interpolate(pts_left, 3, False, 0)[0]
    gate_crv_right = ghcomp.Interpolate(pts_right, 3, False, 0)[0]
    gate_crv_left.Reverse()
    gate_crv_right.Reverse()

# Create gate curve
gate_crv_top = ghcomp.JoinCurves(curves=[gate_crv_left, gate_crv_right], preserve=False)
gate_end_pts = ghcomp.EndPoints(curve=gate_crv_top)
gate_crv_bottom = rg.Line(gate_end_pts[0], gate_end_pts[1])
gate_crv = ghcomp.JoinCurves(curves=[gate_crv_top, gate_crv_bottom], preserve=False)

# Define gate
for vertex in wall.mesh.vertices():
    # get x and z coordinates from vertex
    x_val = wall.mesh.vertex_attribute(vertex, name='x')
    z_val = wall.mesh.vertex_attribute(vertex, name='z')

    # calculate distance
    gate_rel_x = gate_x-x_val
    gate_rel_z = z_val

    #in order to avoid division by zero. should be cleaned up ASAP
    if gate_rel_x==0:
        gate_rel_x=0.000000001

    #this boolean will be set to True if vertex lies inside the gate 
    in_gate_vertex = False

    #for each gate tpye, the a corresponding function is called to check
    # to check if vertex lies inside the gate
    if gate_type=="circular":
        if wall.in_gate_circular(gate_size, gate_rel_x,gate_rel_z):
            in_gate_vertex = True
    elif gate_type=="persian":
        if wall.in_gate_persian(gate_size, gate_rel_x,gate_rel_z):
            in_gate_vertex = True
    elif gate_type=="triangular":
        if wall.in_gate_triangular(gate_size, gate_rel_x,gate_rel_z):
            in_gate_vertex = True
    elif gate_type=="catenary":
        if wall.in_gate_catenary(gate_size, gate_rel_x, gate_rel_z, cat_a):
            in_gate_vertex = True
    elif gate_type=="interactive":
        point = rh.Geometry.Point3d(wall.mesh.vertex_attribute(vertex, name='x'), 0, wall.mesh.vertex_attribute(vertex, name='z') )
        point_in_gate = ghcomp.PointInCurve(point,gate_crv).relationship
        if point_in_gate == 2 or point_in_gate == 1: 
            in_gate_vertex = True

    if in_gate_vertex:
        # store new vertex attribute with the distance to the gate center
        wall.mesh.vertex_attribute(vertex,"x_disp", value=-gate_rel_x)
        wall.mesh.vertex_attribute(vertex,"z_disp", value=gate_rel_z)
        #in_gate flag indicates that this vertex lies in the gate
        wall.mesh.vertex_attribute(vertex,"in_gate", value=True)
        wall.gate_points.append(vertex)

# Create wall curve above gate
gate_left_pts = ghcomp.EndPoints(curve=gate_crv_left)
if gate_left_pts[0].Z &gt; gate_left_pts[1].Z:
    gate_top_pt = gate_left_pts[0]
else:
    gate_top_pt = gate_left_pts[1]
wall_crv_middle = rg.Line(gate_top_pt, rg.Point3d(gate_top_pt.X, gate_top_pt.Y, wall.height))</item>
