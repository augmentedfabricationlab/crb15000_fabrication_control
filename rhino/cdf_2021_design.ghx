                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import ghpythonlib.components as ghcomp
import math



# planes gate left
tsl = crv_gate_left.DivideByLength(avg_layer_height, True)
div_count = len(tsl)
tsc = crv_gate_left.DivideByCount(div_count, True)
planes_gate_left = [crv_gate_left.PerpendicularFrameAt(t)[1] for t in tsc]

# planes gate right
tsc = crv_gate_right.DivideByCount(len(tsl), True)
planes_gate_right = [crv_gate_right.PerpendicularFrameAt(t)[1] for t in tsc]

# planes wall left
tsl = crv_wall_left.DivideByLength(avg_layer_height, True)
planes_wall_left = [crv_wall_left.PerpendicularFrameAt(t)[1] for t in tsl]

# planes wall right
tsr = crv_wall_right.DivideByLength(avg_layer_height, True)
planes_wall_right = [crv_wall_right.PerpendicularFrameAt(t)[1] for t in tsr]

top_wall_left = rg.Curve.Split(crv_wall_left, t=tsl[len(planes_gate_left)-1])[1]
top_wall_right = rg.Curve.Split(crv_wall_right, t=tsr[len(planes_gate_right)-1])[1]

#planes_wall_left_top = planes_wall_left[len(planes_gate_left):]
#planes_wall_right_top = planes_wall_right[len(planes_gate_right):]

# planes gate top
#tsc = crv_gate_top.DivideByCount(len(planes_wall_right_top), False)
#planes_gate_top = [crv_gate_top.PerpendicularFrameAt(t)[1] for t in tsc]

tsc = crv_gate_top.DivideByLength(max_layer_height, True)
planes_gate_top = [crv_gate_top.PerpendicularFrameAt(t)[1] for t in tsc[1:]]

twl = top_wall_left.DivideByCount(len(planes_gate_top)+1, False)
twr = top_wall_right.DivideByCount(len(planes_gate_top)+1, False)
planes_wall_left_top = [top_wall_left.PerpendicularFrameAt(t)[1] for t in twl]
planes_wall_right_top = [top_wall_right.PerpendicularFrameAt(t)[1] for t in twr]


class BlendFunction():
    def __init__(self, func_type, count, blend_layers=0):
        self.count = count
        self.blend_layers = int(min(blend_layers, count/2))
        self.func_type = func_type
        self.blend_type = {
            "linear" : self.blend_linear,
            "cosine" : self.blend_cosine
        }
    
    def get_value(self, value):
        return self.blend_type[self.func_type](value)
    
    def blend_cosine(self, value):
        if self.blend_layers != 0:
            if value &lt;= self.blend_layers:
                step = (math.pi/self.blend_layers)*value
            elif value &gt;= self.count-self.blend_layers:
                step = math.pi + (math.pi/self.blend_layers)*(value-(self.count-self.blend_layers))
            else:
                step = math.pi
        else:
            step = (2*math.pi/self.blend_layers)*value
        blendfactor = (math.cos(math.pi+step)+1)/2
        return blendfactor

    def blend_linear(self, value):
        blendfactor = (1/self.count)*value
        return blendfactor

def generate_crvs(planes_gate, planes_wall, region="left"):
    base_tangent = rg.Vector3d(1,0,0)
    if "left" in region:
        mult=-1
    elif "right" in region:
        mult = 1

    tangent_start_min = mult*base_tangent
    tangent_start_max = tangent_start_min.Clone()
    
    if region=="left" or region=="right":
        b_func = BlendFunction("cosine", len(planes_gate), blend_layers)
        tangent_start_max.Rotate(math.radians(-mult*angle_constraint), rg.Vector3d.YAxis)
    elif region=="topleft" or region=="topright":
        b_func = BlendFunction("linear", len(planes_gate), blend_layers)
        tangent_start_min.Rotate(math.radians(-mult*angle_constraint), rg.Vector3d.YAxis)
    
    crvs = []
    
    for i in range(len(planes_gate)):
        plane_gate = planes_gate[i]
        plane_wall = planes_wall[i]
        
        vertices = [plane_gate.Origin, plane_wall.Origin]
        
        normal = mult*plane_gate.XAxis
        
        if region!="topleft" and region!="topright":
            if i&lt;len(planes_gate)/2:
                tangent_start = b_func.get_value(i)*normal + (1-b_func.get_value(i))*tangent_start_min
            else:
                tangent_start = b_func.get_value(i)*normal + (1-b_func.get_value(i))*tangent_start_max
            
        else:
            tangent_start = b_func.get_value(i)*normal + (1-b_func.get_value(i))*tangent_start_min
        
        tangent_end = mult*plane_wall.XAxis
        c, c_len, c_domain = ghcomp.Interpolatet(vertices, tangent_start, tangent_end, knotstyle=0)
        crvs.append(c)

    return crvs

crvs_region_left = generate_crvs(planes_gate_left, planes_wall_left, region="left")
crvs_region_right = generate_crvs(planes_gate_right, planes_wall_right, region="right")

crvs_region_left_top = generate_crvs(planes_gate_top, planes_wall_left_top, region="topleft")
crvs_region_right_top = generate_crvs(planes_gate_top, planes_wall_right_top, region="topright")

crvs_region_top = []
for i in range(len(crvs_region_left_top)):
    crvs = [crvs_region_left_top[i], crvs_region_right_top[i]]
    
    crv_joined = ghcomp.JoinCurves(crvs, False)
    crvs_region_top.append(crv_joined)</item>
