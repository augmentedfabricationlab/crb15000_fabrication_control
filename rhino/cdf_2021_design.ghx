def create_closed_curves(crvs, mir_crvs, offset):
    new_crvs = []
    for i,crv in enumerate(crvs):
        crv_endpoints = ghcomp.EndPoints(crv)
        mir_crv_endpoints = ghcomp.EndPoints(mir_crvs[i])
        #if Vector.from_start_end(crv_endpoints[0], mir_crv_endpoints[0]).length&gt;offset:
        #    mir_crv_endpoints.reverse()
        transverse_0 = rg.Line(mir_crv_endpoints[0], crv_endpoints[0])
        transverse_1 = rg.Line(crv_endpoints[1], mir_crv_endpoints[1])
        closed_crv = ghcomp.JoinCurves([crv, transverse_1, mir_crvs[i], transverse_0], False)
        new_crvs.append(closed_crv)
    return new_crvs

def get_lengths(curves):
    length = 0
    for crv in curves:
        length += crv.GetLength()*scale
    return length

